{
  "exec": {
    "commands": [
      {
        "command": "tombi format - --stdin-filename {{file_path}}",
        "exts": ["toml"]
      },
      {
        "command": "shfmt -i 0 -ln bash -bn -ci -filename {{file_path}}",
        "exts": ["sh", "bash"]
      },
      {
        "command": "just --dump --justfile {{file_path}}",
        "fileNames": ["justfile"]
      },
      { "command": "rustfmt", "exts": ["rs"] },
      { "command": "sort-package-json --stdin", "fileNames": ["package.json"] },
      {
        "command": "svgo --indent -1 --multipass --pretty -q -i - -o -",
        "exts": ["svg"]
      }
    ]
  },
  "plugins": [
    "https://plugins.dprint.dev/exec-0.6.0.json@a054130d458f124f9b5c91484833828950723a5af3f8ff2bd1523bd47b83b364"
  ]
}
